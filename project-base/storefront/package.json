{
    "name": "shopsys-storefront",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "analyze": "ANALYZE=true next build",
        "build": "next build",
        "check": "concurrently \"pnpm run typecheck\" \"pnpm run lint\"",
        "check--fix": "concurrently \"pnpm run typecheck\" \"pnpm run lint--fix\"",
        "compile-tailwind-for-admin": "tailwindcss -i styles/globals.css -o ./public/tailwind-for-admin/style.css --minify",
        "dev": "NODE_OPTIONS='--inspect=0.0.0.0:9229' next dev",
        "format": "prettier --config ./prettier.config.js --write '**/*.{js,ts,tsx,json}'",
        "format-check": "prettier --config ./prettier.config.js --check '**/*.{js,ts,tsx,json}'",
        "gql": "pnpm run gql:docs && pnpm run gql:codegen && node schema-compress.js",
        "gql:codegen": "graphql-code-generator --config codegen-config.ts",
        "gql:docs": "graphql-markdown schema.graphql > graphql/docs/schema.md",
        "lint": "pnpm run format-check && eslint '**/*.{js,ts,tsx,json}'",
        "lint--fix": "pnpm run format && eslint '**/*.{js,ts,tsx,json}' --fix",
        "start": "next start",
        "translate": "i18next --config config/i18next-parser.config.js",
        "typecheck": "tsc --pretty --noEmit --project tsconfig.json",
        "test": "vitest",
        "test--update": "vitest -u",
        "test--no-watch": "vitest run"
    },
    "dependencies": {
        "@floating-ui/react": "~0.27.16",
        "@hookform/resolvers": "~2.6.1",
        "@next/bundle-analyzer": "~15.4.7",
        "@sentry/nextjs": "~9.38.0",
        "@urql/devtools": "~2.0.3",
        "@urql/exchange-auth": "~2.1.6",
        "@urql/exchange-graphcache": "~6.4.1",
        "cookies-next": "~4.3.0",
        "crypto-js": "~4.2.0",
        "dayjs": "~1.11.18",
        "framer-motion": "^11.18.2",
        "google-map-react": "^2.2.5",
        "graphql": "~16.8.2",
        "graphql-tag": "~2.12.6",
        "isomorphic-dompurify": "^2.26.0",
        "js-base64": "~3.7.8",
        "jwt-decode": "~4.0.0",
        "lz-string": "~1.5.0",
        "next": "~15.4.7",
        "next-translate": "~2.6.2",
        "next-translate-plugin": "~2.6.2",
        "next-urql": "~5.0.2",
        "nprogress": "~0.2.0",
        "react": "~18.2.0",
        "react-dom": "~18.2.0",
        "react-dropzone": "^14.3.8",
        "react-error-boundary": "~4.1.2",
        "react-hook-form": "~7.35.0",
        "react-infinite-scroll-component": "~6.1.0",
        "react-merge-refs": "~2.1.1",
        "react-remove-scroll": "^2.7.1",
        "react-swipeable": "~7.0.2",
        "react-tabs": "~6.1.0",
        "react-toastify": "~10.0.6",
        "redis": "~4.7.1",
        "supercluster": "^8.0.1",
        "tailwind-merge": "~3.0.2",
        "urql": "~4.0.7",
        "use-supercluster": "^1.2.0",
        "uuid": "~9.0.1",
        "wonka": "~6.3.5",
        "yup": "~1.6.1",
        "zustand": "~4.4.7"
    },
    "devDependencies": {
        "@eslint/compat": "^1.3.2",
        "@eslint/eslintrc": "^3.3.1",
        "@eslint/js": "^9.34.0",
        "@graphql-codegen/cli": "~5.0.7",
        "@graphql-codegen/fragment-matcher": "~5.0.2",
        "@graphql-codegen/introspection": "~4.0.3",
        "@graphql-codegen/near-operation-file-preset": "^3.1.0",
        "@graphql-codegen/typescript-operations": "~4.0.1",
        "@graphql-codegen/typescript-urql": "~4.0.1",
        "@mswjs/interceptors": "0.19.0",
        "@tailwindcss/cli": "^4.1.11",
        "@tailwindcss/postcss": "~4.1.12",
        "@testing-library/jest-dom": "^6.8.0",
        "@testing-library/react": "~14.3.1",
        "@testing-library/user-event": "^14.6.1",
        "@trivago/prettier-plugin-sort-imports": "~4.3.0",
        "@types/crypto-js": "~4.2.2",
        "@types/google-map-react": "^2.1.10",
        "@types/node": "~20.11.30",
        "@types/nprogress": "~0.2.3",
        "@types/react": "~18.2.79",
        "@types/react-dom": "~18.2.25",
        "@types/supercluster": "^7.1.3",
        "@types/uuid": "~9.0.8",
        "@typescript-eslint/eslint-plugin": "~8.29.1",
        "@typescript-eslint/parser": "~8.26.1",
        "@urql/introspection": "^1.2.1",
        "@vitejs/plugin-react": "4.2.1",
        "concurrently": "~8.2.2",
        "eslint": "~9.22.0",
        "eslint-config-prettier": "~9.1.2",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-no-relative-import-paths": "~1.6.1",
        "eslint-plugin-react": "~7.37.5",
        "eslint-plugin-react-hooks": "~5.2.0",
        "eslint-plugin-unused-imports": "~4.1.4",
        "globals": "^16.3.0",
        "graphql-markdown": "~7.3.0",
        "i18next-parser": "~9.0.2",
        "jsdom": "~24.0.0",
        "license-checker": "^25.0.1",
        "postcss": "~8.5.6",
        "prettier": "~3.2.5",
        "prettier-plugin-tailwindcss": "~0.6.14",
        "tailwindcss": "~4.1.12",
        "typescript": "~5.3.3",
        "vite-tsconfig-paths": "~4.3.2",
        "vitest": "~0.34.6"
    },
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild",
            "next-translate-plugin"
        ],
        "overrides": {
            "@urql/core": "6.0.1",
            "esbuild@<=0.24.2": ">=0.25.0",
            "vite": "5.4.19",
            "brace-expansion@>=1.0.0 <=1.1.11": "1.1.12",
            "brace-expansion@>=2.0.0 <=2.0.1": "1.1.12",
            "form-data@>=4.0.0 <4.0.4": ">=4.0.4",
            "@eslint/plugin-kit@<0.3.4": ">=0.3.4",
            "tmp@<=0.2.3": ">=0.2.4"
        }
    }
}
