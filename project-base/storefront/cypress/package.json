{
    "name": "cypress-tests-shopsys-platform-storefront",
    "version": "0.1.0",
    "description": "Acceptance tests of Shopsys Platform Storefront",
    "private": true,
    "scripts": {
        "typecheck": "tsc --pretty --noEmit --project tsconfig.json",
        "open-base": "cypress open --env visualRegressionType=base",
        "open-regression": "cypress open --env visualRegressionType=regression",
        "base": "cypress run --env visualRegressionType=base",
        "regression": "cypress run --env visualRegressionType=regression",
        "specific-base": "cypress run --env visualRegressionType=base --spec",
        "specific-regression": "cypress run --env visualRegressionType=regression --spec",
        "selected-regression": "./runSelectedTests.js visualRegressionType=regression",
        "selected-base": "./runSelectedTests.js visualRegressionType=base",
        "generate-snapshots-table": "./generateSnapshotsInfoTable.js",
        "smoke": "cypress run --spec ./smokeTests/smokeTests.cy.ts"
    },
    "devDependencies": {
        "cypress": "^14.5.3",
        "cypress-real-events": "^1.14.0",
        "cypress-visual-regression": "^5.3.0",
        "cypress-wait-for-stable-dom": "^0.1.0",
        "glob": "^11.0.3",
        "inquirer": "^10.1.8",
        "typescript": "^5.1.6",
        "uuid": "^9.0.1"
    }
}
